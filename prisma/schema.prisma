// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model Subject {
  id          String   @id @default(cuid())
  title       String
  content     String
  currentDate DateTime @default(now())

  @@map("subjects")
}

